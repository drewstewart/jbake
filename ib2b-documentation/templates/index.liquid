<!DOCTYPE html>
<html lang="en">

	<!-- import stylesheet includes -->
	{% include "header" %}
	
	<body onload="prettyPrint()">
		<div id="wrap">

			{% include "nav-menu" %}
			
			<div id="hero">
			  <div class="container">
			    <div class="row">
			      <div class="col-md-24 hero-text">
			        <h1>IB2B Service Specifications</h1>
			        <br />
			        <br />
			      </div>
			    </div>
			    <div class="row">
			    	<div class="col-md-offset-3 col-md-6">
			    		<a href="/getting-started" class="btn btn-info-alt btn-block">LEARN MORE ABOUT WORKING WITH SPECS&nbsp;<span class="glyphicon glyphicon-menu-right"></span></a>
			    	</div>
			    </div>
			  </div>
			</div>
			
			<div class="container">
				<div class="row">
				
					{% for item in published_interface-specifications %}
					<div class="col-md-3 text-center">
						<a href="{{ item.uri }}" class="link-block">
							<div>{{ item.title }}&nbsp;{{ item.version }}</div>
						</a>
					</div>
					{% endfor %}
					
				</div>
			</div>
		</div>

		{% include "footer" %}

	</body>
</html>